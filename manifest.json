{
  "manifest_version": 2,
  "name": "KeyboardMode",
  "description": "This extension allows you to open links with your Keyboard.",
  "icons": {"128": "icon.png"},
  "version": "1.0",
  "options_page": "options.html",

  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js" : ["lib/keyboard_utils.js", "lib/dom_utils.js", "link_hints.js", "command.js"],
		"css": ["vimium.css"],
		"run_at": "document_start",
		"all_frames": true
	}
	]
}
